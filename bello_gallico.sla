        <PAGEOBJECT XPOS="136.0008" YPOS="26699.2357910828" OwnPage="63" ItemID="1951279092" PTYPE="4" WIDTH="531" HEIGHT="313.070063694267" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L531 0 L531 313.07 L0 313.07 L0 0 Z" copath="M0 0 L531 0 L531 313.07 L0 313.07 L0 0 Z" gXpos="136.0008" gYpos="26699.2357910828" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <para/>
                <para/>
                <ITEXT CH="#8 Orīginālis"/>
                <para PARENT="Chapter Heading"/>
                <para PARENT="Chapter Heading"/>
                <ITEXT CH="A Surprise Attack!"/>
                <para PARENT="Chapter Heading"/>
                <para/>
                <tab/>
                <ITEXT CH="At Caesar, etsī nōndum eōrum cōnsilia cognōverat, tamen et ex ēventū nāvium suārum et ex eō quod obsidēs dare intermīserant fore"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=" id quod accidit suspicābātur. Itaque ad omnēs cāsūs subsidia comparābat. Nam et frūmentum ex agrīs cotīdiē in castra cōnferēbat et, quae gravissimē adflīctae"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" erant nāvēs, eārum māteriā atque aere ad reliquās reficiendās"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" ūtēbātur et quae ad eās rēs erant ūsuī ex continentī comportārī iubēbat. Itaque, cum summō studiō ā mīlitibus administrārētur, XII nāvibus āmissīs, reliquīs ut nāvigārī satis commodē posset effēcit."/>
                <trail PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="757.0008" YPOS="26680.104" OwnPage="64" ItemID="1951286468" PTYPE="4" WIDTH="531" HEIGHT="701.526695541412" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L531 0 L531 701.527 L0 701.527 L0 0 Z" copath="M0 0 L531 0 L531 701.527 L0 701.527 L0 0 Z" gXpos="757.0008" gYpos="26680.104" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="ā: from, out of, by, since"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="accido, accidere, accidi, -: fall upon/down/to/at or near, descend, alight; happen, occur; happen to (DAT); infinitive"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="ad: to, towards, at, according to"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="adflīctae: having been striken; plural feminine nominative perfect passive participle from "/>
                <para PARENT="Vocab"/>
                <ITEXT CH="administrārētur: he (might) have been being attended to; 3rd person singular imperfect passive subjunctive from administrō, administrāre, administrāvī, administrātus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="aere: copper; singular neuter ablative from aes, aeris"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="agrīs: fields; plural masculine  from ager, agrī"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="āmissīs: having been lost, dismissed; plural dative perfect passive participle from āmittō, āmittere, āmīsī, āmissus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="at: and"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="atque: and"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="caesar: Caesar; singular masculine nominative from Caesar, Caesaris"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="castra: forts; plural neuter nominative from castrum, castrī"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="cāsus: fall, downward movement; singular masculine nominative perfect passive participle from cadō, cadere, cecidī, cāsus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="cōgnōverat: they had recognized, learned; 3rd person singular pluperfect active from cognosco, cognoscere, cognovi, cognitus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="commodē: suitably"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="comparābat: he prepared; 3rd person singular imperfect active from comparō, comparāre, comparāvī, comparātus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="comportārī: to be brought together; passive infinitive from comportō, comportāre, comportāvī, comportātus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="cōnferēbat: he was talking; 3rd person singular imperfect active from cōnferō, cōnferre, contulī, collātus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="cōnsilia: plans, advice, councils; plural neuter nominative from consilium, consili(i)"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="continentī: continent; singular dative present participle from contineō, continēre, continuī, contentus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="cotīdiē: daily"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="cum: with"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="dare: to give;  person  present active from do, dare, dedi, datus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="eārum: their; plural feminine genitive from is, ea, id"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="eās: their; plural feminine accusative from is, ea, id"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="effēcit: he achieved, produced; 3rd person singular perfect active from efficiō, efficere, effēcī, effectus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="eō: I go; 1st person singular present active from eo, ire, ivi(ii), itus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="eōrum: their; plural masc. or neuter. genitive"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="erant: they were; 3rd person plural imperfect active from sum, esse, fuī, futūrus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="et: and"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="etsī: even so"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="ēventū: event; singular neuter ablative from ēveniō, ēvenīre, ēvēnī, ēventus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="ex: out of, from"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="fore: to be;  person  future active from sum, esse, fuī, futūrus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="frūmentum: corn, grain; singular neuter nominative from frūmentum, frūmentī"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="gravissimē: very gravely"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="id: it"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="in: into (with accusative), in (with ablative)"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="intermīserant: they had interrupted; 3rd person plural pluperfect active from intermittō, intermittere, intermīsī, intermissus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="itaque: so then, therefore"/>
                <para PARENT="Vocab"/>
