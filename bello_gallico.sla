                <ITEXT CH="#3 Orīginālis"/>
                <para PARENT="Chapter Heading"/>
                <para PARENT="Chapter Heading"/>
                <ITEXT CH="The Preparations of the Helvetii"/>
                <para PARENT="Chapter Heading"/>
                <para/>
                <tab/>
                <ITEXT CH="Ea rēs est"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=" Helvētiīs per indicium ēnūntiāta. Mōribus suīs"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" Orgetoricem ex vinculīs causam dīcere coēgērunt; damnātum poenam sequī oportēbat"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=", ut ignī cremārētur. Diē cōnstitūtā"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=" causae dictiōnis Orgetorix ad iūdicium omnem suam familiam, ad hominum mīlia decem"/>
                <ITEXT FEATURES="inherit superscript" CH="5"/>
                <ITEXT CH=", undique coēgit, et omnēs clientēs obaerātōsque suōs, quōrum magnum numerum habēbat, eōdem condūxit; per eōs nē causam dīceret"/>
                <ITEXT FEATURES="inherit superscript" CH="6"/>
                <ITEXT CH=" sē ēripuit. Cum cīvitās ob eam rem incitātā"/>
                <ITEXT FEATURES="inherit superscript" CH="7"/>
                <ITEXT CH=" armīs iūs suum exsequī cōnārētur multitūdinemque hominum ex agrīs magistrātūs cōgerent, Orgetorix mortuus est"/>
                <ITEXT FEATURES="inherit superscript" CH="8"/>
                <ITEXT CH="; neque abest suspīciō, ut Helvētiī arbitrantur, quīn ipse sibi mortem cōnscīverit"/>
                <ITEXT FEATURES="inherit superscript" CH="9"/>
                <ITEXT CH="."/>
                <para PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="136.0008" YPOS="17546.3313936306" OwnPage="41" ItemID="1878144276" PTYPE="4" WIDTH="531" HEIGHT="299.006369426752" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L531 0 L531 299.006 L0 299.006 L0 0 Z" copath="M0 0 L531 0 L531 299.006 L0 299.006 L0 0 Z" gXpos="136.0008" gYpos="17546.3313936306" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <para/>
                <tab/>
                <ITEXT CH="Post eius mortem nihilō"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=" minus Helvētiī id quod cōnstituerant facere cōnantur, ut ē fīnibus suīs exeant. Ubi iam sē ad eam rem parātōs"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" esse arbitrātī sunt, oppida sua omnia, numerō ad duodecim, vīcōs ad quadringentōs, reliqua prīvāta aedificia incendunt; frūmentum omne, praeter quod sēcum portātūrī erant, combūrunt, ut domum reditiōnis"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" spē sublātā parātiōrēs ad omnia perīcula subeunda essent; trium mēnsum molita cibāria sibi quemque domō efferre iubent. Persuādent Rauracīs et Tulingīs et Latobrigīs fīnitimīs, utī"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=" eōdem ūsī cōnsiliō oppidīs suīs vīcīsque exustīs ūna cum iīs proficīscantur, Boiōsque, quī trāns Rhēnum incoluerant et in agrum Nōricum trānsierant Norēiamque oppugnābant, receptōs ad sē sociōs sibi adscīscunt. "/>
                <para PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
