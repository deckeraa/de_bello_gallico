                <para PARENT="Chapter Heading"/>
                <para/>
                <tab/>
                <ITEXT CH="Hīs rēbus pāce"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=" cōnfirmātā, post diem quārtum quam"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" est in Britanniam ventum nāvēs XVIII, dē quibus suprā dēmōnstrātum"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" est, quae equitēs sustulerant, ex superiōre portū lēnī ventō solvērunt. Quae cum adpropinquārent Britanniae et ex castrīs vidērentur, tanta tempestās subitō coorta est ut nūlla eārum cursum tenēre posset, sed aliae eōdem unde erant"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=" profectae referrentur, aliae ad īnferiōrem partem īnsulae, quae est propius sōlis occāsum"/>
                <ITEXT FEATURES="inherit superscript" CH="5"/>
                <ITEXT CH=", magnō suō cum perīculō dēicerentur; quae tamen ancorīs iactīs cum flūctibus complērentur"/>
                <ITEXT FEATURES="inherit superscript" CH="6"/>
                <ITEXT CH=", necessāriō"/>
                <ITEXT FEATURES="inherit superscript" CH="7"/>
                <ITEXT CH=" adversā nocte in altum prōvectae continentem petiērunt."/>
                <trail PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
