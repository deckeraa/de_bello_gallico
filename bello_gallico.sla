            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Galliā habet trēs partēs."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Belgae incolunt (vīvunt) in ūnā parte, Aquītānī incolunt in aliā parte, et Gallī incolunt in tertiā parte."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Gallī appellantur Celtae in linguā eōrum."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Belgae, Aquītānī, et Gallī habent dīversās linguās, īnstitūta, et legēs."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Flūmen, cuius nōmen est Garumna, dīvidit Gallōs ab Aquītānīs."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Flūmina, quōrum nōmina sunt Matrona et Sēquana, dīvidunt Gallōs ab Belgīs."/>
                <trail PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="136.0008" YPOS="1739.00636942675" OwnPage="3" ItemID="1838403716" PTYPE="4" WIDTH="531" HEIGHT="384.305732484076" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L531 0 L531 384.306 L0 384.306 L0 0 Z" copath="M0 0 L531 0 L531 384.306 L0 384.306 L0 0 Z" gXpos="136.0008" gYpos="1739.00636942675" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Belgae sunt fortiōrēs quam Gallī et Aquītānī."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Prōvinciae Belgārum sunt longissimē ā cultū."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Mercātōrēs nōn saepe important rēs ad Belgās."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Germānī incolunt prope Belgās."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Belgae et Germānī saepe gerunt bellum."/>
                <para PARENT="Latin"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Helvētiī quoque sunt fortiōrēs quam reliquī Gallī."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Helvētiī gerunt bellum contrā Germānōs cottīdiē."/>
                <para PARENT="Latin"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Galliā est inter Rhodanum, Garumnam, Ōceanum, et fīnēs Belgārum."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Galliā quoque attingit flūmen Rhēnum, quī dīvidit Gallī ab Sēquanīs et Helvētiīs."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Belgae sunt in septentriōnem et orientem sōlem."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Aquītānī sunt inter occāsum sōlis et septentriōnēs."/>
                <para PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
