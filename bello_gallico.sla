        <PAGEOBJECT XPOS="136.0008" YPOS="26699.2357910828" OwnPage="63" ItemID="1951279092" PTYPE="4" WIDTH="531" HEIGHT="313.070063694267" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L531 0 L531 313.07 L0 313.07 L0 0 Z" copath="M0 0 L531 0 L531 313.07 L0 313.07 L0 0 Z" gXpos="136.0008" gYpos="26699.2357910828" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <para/>
                <para/>
                <ITEXT CH="#8 Orginlis"/>
                <para PARENT="Chapter Heading"/>
                <para PARENT="Chapter Heading"/>
                <ITEXT CH="A Surprise Attack!"/>
                <para PARENT="Chapter Heading"/>
                <para/>
                <tab/>
                <ITEXT CH="At Caesar, ets nndum erum cnsilia cognverat, tamen et ex vent nvium surum et ex e quod obsids dare intermserant fore"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=" id quod accidit suspicbtur. Itaque ad omns css subsidia comparbat. Nam et frmentum ex agrs cotdi in castra cnferbat et, quae gravissim adflctae"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" erant nvs, erum mteri atque aere ad reliqus reficiends"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" tbtur et quae ad es rs erant su ex continent comportr iubbat. Itaque, cum summ studi  mlitibus administrrtur, XII nvibus misss, reliqus ut nvigr satis commod posset effcit."/>
                <trail PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="757.0008" YPOS="26680.104" OwnPage="64" ItemID="1951286468" PTYPE="4" WIDTH="531" HEIGHT="701.526695541412" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L531 0 L531 701.527 L0 701.527 L0 0 Z" copath="M0 0 L531 0 L531 701.527 L0 701.527 L0 0 Z" gXpos="757.0008" gYpos="26680.104" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH=": from, out of, by, since"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="accido, accidere, accidi, -: fall upon/down/to/at or near, descend, alight; happen, occur; happen to (DAT); infinitive"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="ad: to, towards, at, according to"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="adflctae: having been striken; plural feminine nominative perfect passive participle from "/>
                <para PARENT="Vocab"/>
                <ITEXT CH="administrrtur: he (might) have been being attended to; 3rd person singular imperfect passive subjunctive from administr, administrre, administrv, administrtus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="aere: copper; singular neuter ablative from aes, aeris"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="agrs: fields; plural masculine  from ager, agr"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="misss: having been lost, dismissed; plural dative perfect passive participle from mitt, mittere, ms, missus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="at: and"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="atque: and"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="caesar: Caesar; singular masculine nominative from Caesar, Caesaris"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="castra: forts; plural neuter nominative from castrum, castr"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="csus: fall, downward movement; singular masculine nominative perfect passive participle from cad, cadere, cecid, csus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="cgnverat: they had recognized, learned; 3rd person singular pluperfect active from cognosco, cognoscere, cognovi, cognitus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="commod: suitably"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="comparbat: he prepared; 3rd person singular imperfect active from compar, comparre, comparv, compartus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="comportr: to be brought together; passive infinitive from comport, comportre, comportv, comporttus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="cnferbat: he was talking; 3rd person singular imperfect active from cnfer, cnferre, contul, colltus"/>
                <para PARENT="Vocab"/>
