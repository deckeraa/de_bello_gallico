        <PAGEOBJECT XPOS="136.0008" YPOS="6730.58598726115" OwnPage="15" ItemID="2001528324" PTYPE="4" WIDTH="531" HEIGHT="320.407643312102" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L531 0 L531 320.408 L0 320.408 L0 0 Z" copath="M0 0 L531 0 L531 320.408 L0 320.408 L0 0 Z" gXpos="136.0008" gYpos="6730.58598726115" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
