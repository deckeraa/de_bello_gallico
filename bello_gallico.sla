                <ITEXT CH="Caesarī cum id nūntiātum"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=" esset, eōs"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" per prōvinciam nostram iter facere cōnārī, mātūrat"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" ab urbe proficīscī et quam"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=" maximīs potest itineribus in Galliam ulteriōrem contendit et ad Genāvam pervēnit. Prōvinciae"/>
                <ITEXT FEATURES="inherit superscript" CH="5"/>
                <ITEXT CH=" tōtī quam maximum potest mīlitum numerum imperat (erat omnīnō in Galliā ulteriōre legiō ūna), pontem, quī erat ad Genāvam, iubet rescindī. Ubi dē eius adventū Helvētiī certiōrēs"/>
                <ITEXT FEATURES="inherit superscript" CH="6"/>
                <ITEXT CH=" factī sunt, lēgātōs ad eum mittunt nōbilissimōs cīvitātis, cuius lēgātiōnis Nammēius et Verucloetius prīncipem locum obtinēbant, quī dīcerent sibi esse in animō sine ūllō maleficiō iter per prōvinciam facere, proptereā quod aliud iter habērent nūllum: rogāre ut eius voluntāte"/>
                <ITEXT FEATURES="inherit superscript" CH="8"/>
                <ITEXT CH=" id sibi facere liceat."/>
                <trail PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
